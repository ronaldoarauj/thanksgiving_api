{
    "openapi": "3.0.0",
    "info": {
      "title": "Nome da Sua API",
      "version": "1.0.0",
      "description": "Descrição da sua API"
    },
    "components": {
      "securitySchemes": {
        "bearerAuth": {
          "type": "http",
          "scheme": "bearer",
          "bearerFormat": "JWT"
        }
      }
    },
    "security": [
      {
        "bearerAuth": []
      }
    ],
    "paths": {
      "/api/users": {
        "get": {
          "summary": "Obtém a lista de usuários",
          "security": [
            {
              "bearerAuth": []
            }
          ],
          "responses": {
            "200": {
              "description": "Sucesso",
              "content": {
                "application/json": {
                  "example": {
                    "users": []
                  }
                }
              }
            }
          }
        }
      },
      "/api/users/{id}": {
        "get": {
          "summary": "Obtém um usuário por ID",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              },
              "description": "ID do usuário a ser recuperado"
            }
          ],
          "security": [
            {
              "bearerAuth": []
            }
          ],
          "responses": {
            "200": {
              "description": "Sucesso",
              "content": {
                "application/json": {
                  "example": {
                    "user": {}
                  }
                }
              }
            },
            "404": {
              "description": "Usuário não encontrado"
            }
          }
        }
      }
 
    }
  }
  